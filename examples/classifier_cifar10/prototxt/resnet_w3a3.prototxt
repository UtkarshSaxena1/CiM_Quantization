main_file: "examples/classifier_cifar10/main_lsq.py"
arch: "resnet20"
model_source: Local
log_name: "logs"
debug: false
data: "./CIFAR-10"
lr: 0.001
epochs: 300
batch_size: 128
workers: 8
print_freq: 40
evaluate: true
pretrained: true
seed: 0
gpu_id: ANY


nbits_w: 3
nbits_a: 3
nbits_alpha: 8
wbitslice: 1
abitslice: 1
xbar: 128
adcbits: 8


lr_scheduler: CosineAnnealingLR


optimizer: SGD
sgd {
  weight_decay: 1e-4
  momentum: 0.9
}


